package juegos.juego3.src {import com.demonsters.debugger.MonsterDebugger;import events.JuegoEvent;import flash.display.MovieClip;import flash.display.Sprite;import flash.events.Event;import flash.text.TextField;public class Juego3 extends MovieClip {    private var _this:Juego3;    public function Juego3() {        super();        _this =  this;        _this.addEventListener(Event.ADDED_TO_STAGE, init);    }    private function init(e:Event):void    {        _this.removeEventListener(Event.ADDED_TO_STAGE, init);        dispatchEvent(new JuegoEvent(JuegoEvent.JUEGO_CARGADO));    }}}