package assets {import assets.IcoAmigo;import flash.display.MovieClip;    import com.hexagonstar.util.debug.Debug;			public class MaderitaFoto extends MovieClip {        private var user_id:String;        private var foto_paMarco:IcoAmigo;        private var _this:MaderitaFoto;        private var contFoto:ContenedorFoto_mc;        private var txtFoto:TxtFotoAmigos;        private var nombreAmig:Array=new Array();				public function MaderitaFoto() {// constructor code            _this = this;		}        public function ponerFoto(amigo:Object):void{            user_id=amigo.id;            foto_paMarco=new IcoAmigo();            foto_paMarco.cargarImagen(user_id);            foto_paMarco.x=0;            foto_paMarco.y=0;            contFoto=new ContenedorFoto_mc();            contFoto.x = 10;            contFoto.y = 17;            _this.addChild(contFoto);            contFoto.addChild(foto_paMarco);            txtFoto=new TxtFotoAmigos();            txtFoto.x = 46.25;            txtFoto.y = 90.3;            nombreAmig=amigo.namename.split(" ");            txtFoto.txt_foto.text=nombreAmig[0];           _this.addChild(txtFoto);            MovieClip(this.getChildByName('contenedorFoto_mc')).addChild(foto_paMarco);        }    }	}